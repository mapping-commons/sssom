    {
      "name": "curie_map",
      "definition_uri": "https://w3id.org/sssom/curie_map",
      "description": "A dictionary that contains prefixes as keys and their URI expansions as values.",
      "from_schema": "https://w3id.org/sssom/schema/",
      "see_also": [
